

<% if logged_in? %>
  <%= link_to "Aubrey's Profile", user_path(current_user) %>
  <%= form_tag ({controller: 'sessions', action: 'destroy', method: 'POST'}) do %>
    <input type="submit" value="Log Out">
    <% end %>
<% else %>
    <%= link_to "Sign Up", new_user_path %>
    <%= link_to "Log In", signin_path %>
<% end %>
